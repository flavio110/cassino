        return t.v4shim.enabled && n.push(new _((function (n, o) {
            P(c(t, {
                addOn: "-v4-shims",
                minify: t.minify.enabled
            }), e).then((function (t) {
                n(I(t, r(r({}, e), {}, {
                    id: "fa-v4-shims"
                })))
            })).catch(o)
        }))), n.push(new _((function (n, o) {
            P(c(t, {
                minify: t.minify.enabled
            }), e).then((function (t) {
                var o = I(t, r(r({}, e), {}, {
                    id: "fa-main"
                }));
                n(function (t, e) {
