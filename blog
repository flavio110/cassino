        <b:with value='[&quot;Completo&quot;,&quot;Em andamento&quot;,&quot;Em andamento&quot;]' var='checkStatus'>
      <b:loop values='data:post.labels' var='type'>
          <b:loop values='data:checkType' var='typeLabel'>
            <b:if cond='data:typeLabel == data:type.name'>
              <b:loop values='data:post.labels' var='status'>
          <b:loop values='data:checkStatus' var='statusLabel'>
            <b:if cond='data:statusLabel == data:status.name'>
              <b:loop values='data:post.labels' var='episode'>
                <b:if cond='data: + &quot;String: &quot; + data:episode.name contains &quot;Eps.&quot;'>
