<b:section class='sidebar.popular' id='pop2' name='pop2' showaddelement='no'>
  <b:widget id='PopularPosts2' locked='false' title='Mensalmente' type='PopularPosts' version='2' visible='true'>
    <b:widget-settings>
      <b:widget-setting name='numItemsToShow'>7</b:widget-setting>
      <b:widget-setting name='showThumbnails'>true</b:widget-setting>
      <b:widget-setting name='showSnippets'>true</b:widget-setting>
      <b:widget-setting name='timeRange'>LAST_MONTH</b:widget-setting>
    </b:widget-settings>
    <b:includable id='main' var='this'>
  <b:include name='widget-title'/>
  <div class='widget-content popular-posts'>
    <ul>
<b:loop index='x' values='data:view.isMultipleItems ? data:posts where (p =&gt; p.labels any (l =&gt; l.name == &quot;Séries&quot;)) : data:posts' var='post'>
        <b:if cond='data:post.labels any (l =&gt; l.name == &quot;Séries&quot;)'>
      <li>
        <b:if cond='data:showThumbnails == &quot;false&quot;'>
          <b:if cond='data:showSnippets == &quot;false&quot;'>
            <!-- (1) No snippet/thumbnail -->
            <a expr:href='data:post.href'><data:post.title/></a>
          <b:else/>
            <!-- (2) Show only snippets -->
            <div class='item-title'><a expr:href='data:post.href'><data:post.title/></a>
      </div>
            <div class='item-snippet'><data:post.snippet/></div>
          </b:if>
        <b:else/>
          <b:if cond='data:showSnippets == &quot;false&quot;'>
            <!-- (3) Show only thumbnails -->
            <div class='item-thumbnail-only'>
              <b:if cond='data:post.thumbnail'>
                <div class='item-thumbnail'>
                  <a expr:href='data:post.href' target='_blank'>
                    <img border='0' expr:alt='data:post.title' expr:height='data:thumbnailSize' expr:src='data:post.thumbnail' expr:title='data:post.title' expr:width='data:thumbnailSize'/>
                  </a>
                </div>
              </b:if>
              <div class='item-title'><a expr:href='data:post.url'><data:post.title/></a>
      </div>
            </div>
            <div style='clear: both;'/>
          <b:else/>
            <!-- (4) Show snippets and thumbnails -->
            <div class='item-content'>
      <!--Gambar Kecil-->  
              <div class='bpop'>
      <b:if cond='data:post.featuredImage'>
        <div class='coverpop'>
      <a expr:href='data:post.url'>
        <img class='post-thumbnail lazyload' expr:alt='data:post.title' expr:data-src='data:post.featuredImage' expr:height='data:height' expr:title='data:post.title' expr:width='data:width' loading='lazy' src='data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAMAAAACCAQAAAA3fa6RAAAADklEQVR42mNkAANGCAUAACMAA2w/AMgAAAAASUVORK5CYII='/>
      </a>
        </div>
      </b:if>
<div class='infopop' style='overflow:hidden;'>
              <div class='item-title'><a expr:href='data:post.url'><data:post.title/></a>    
      </div>
              <div class='item-label'>
        <!--Genre Label-->
<span class='tren-tag'><b>Genêro:</b>
<b:loop index='i' values='data:post.labels' var='label'>
